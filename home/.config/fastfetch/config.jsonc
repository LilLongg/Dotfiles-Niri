      "key": "│ └",
      "keyColor": "yellow",
    },
    {
      "type": "wm",
      "key": " WM",
      "keyColor": "blue",
    },
    /**
    {
      "type": "lm",
      "key": "│ ├󰧨",
      "keyColor": "blue"
    },
    **/
    {
      "type": "theme",
      "key": "│ ├󰉼",
      "keyColor": "blue",
    },
    {
      "type": "icons",
      "key": "│ ├󰀻",
      "keyColor": "blue",
    },
    {
      "type": "cursor",
      "key": "│ ├󰇀",
      "keyColor": "blue",
    },
    {
      "type": "font",
      "key": "│ ├",
      "keyColor": "blue",
