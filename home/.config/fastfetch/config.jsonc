    },
    {
      "type": "terminal",
      "key": "│ └",
      "keyColor": "blue",
    },
    /**
    {
      "type": "wallpaper",
      "key": "│ └󰸉",
      "keyColor": "blue"
    },**/
    {
      "type": "host",
      "key": " PC",
      "keyColor": "green",
    },
    {
      "type": "cpu",
      "key": "│ ├",
      "keyColor": "green",
      "temp": true,
      "format": "{1} ({4}) @ {6}",
    },
    {
      "type": "gpu",
      "key": "│ ├󰍛",
      "keyColor": "green",
