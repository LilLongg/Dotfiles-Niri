      "keyColor": "cyan",
    },
    {
      "type": "player",
      "key": "│ ├󰥠",
      "keyColor": "cyan",
    },
    {
      "type": "media",
      "key": "│ └󰝚",
      "keyColor": "cyan",
    },
    {
      "type": "custom",
      "format": "└────────────────────────────────────────────────────────────┘",
